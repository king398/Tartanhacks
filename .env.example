VIDEO_PATH=sample.mp4
IN_STORE_VIDEO_PATH=sample1.mp4
YOLO_MODEL=yolo11s.pt
YOLO_DEVICE=cuda:0
YOLO_FP16=true
YOLO_COMPILE=true
YOLO_TENSORRT=false
# Optional explicit engine path. If empty and YOLO_MODEL is .pt, app uses/creates YOLO_MODEL with .engine suffix.
# YOLO_TRT_ENGINE=yolo26m.engine
# Optional per-camera model/engine overrides
# DRIVE_THRU_YOLO_MODEL=yolo11s.pt
# IN_STORE_YOLO_MODEL=yolo11s.pt
# DRIVE_THRU_YOLO_TRT_ENGINE=yolo11s.engine
# IN_STORE_YOLO_TRT_ENGINE=yolo11s.engine
SAMPLE_FPS=30
CONF_THRESHOLD=0.35
DRIVE_THRU_CONF_THRESHOLD=0.22
IN_STORE_CONF_THRESHOLD=0.15
DRIVE_THRU_COUNT_HOLD_SEC=0.60
IOU_THRESHOLD=0.50
IMG_SIZE=640
# Optional per-camera overrides (fallback to globals above when unset)
# DRIVE_THRU_SAMPLE_FPS=30
# IN_STORE_SAMPLE_FPS=30
# DRIVE_THRU_IOU_THRESHOLD=0.50
# IN_STORE_IOU_THRESHOLD=0.50
# DRIVE_THRU_IMG_SIZE=640
# IN_STORE_IMG_SIZE=640
PEOPLE_PER_CAR=1.5
AVG_SERVICE_TIME_SEC=45
CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000
RECO_FORECAST_HORIZON_MIN=8
RECO_DROP_CADENCE_MIN=4
AVG_TICKET_USD=10.5
ANALYTICS_DB_PATH=analytics.db
ANALYTICS_SAMPLE_INTERVAL_SEC=1.0
